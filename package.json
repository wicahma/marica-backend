{
  "name": "marica_backend",
  "version": "1.0.1",
  "description": "Marica merupakan sebuah platform pembelajara yang berisikan video video interaktif dan juga menarik yang dapat menarik minat anak kecil. Tidak hanya video, tetapi juga Marica menyediakan Quiz yang akan diputar baik itu pada saat video berlangsung, atau pada saat video selesai, sehingga dapat langsung menguji dan juga mengasah skill anak anak ketika telah selesai mengisi soal soal tersebut.",
  "main": "src/index.js",
  "scripts": {
    "debug": "nodemon src/index.js",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wicahma/marica-backend.git"
  },
  "keywords": [
    "backend",
    "marica",
    "anak-anak",
    "permainan",
    "login",
    "JWT",
    "server"
  ],
  "author": "Teguh Dwi Cahya Kusuma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wicahma/marica-backend/issues"
  },
  "homepage": "https://github.com/wicahma/marica-backend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "googleapis": "^112.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "passport-google-oauth20": "^2.0.0",
    "unique-username-generator": "^1.1.3"
  },
  "engines": {
    "node": "18"
  }
